<?xml version='1.0'?>
<sdf version='1.6'>
  <!--
    IMPORTANT (for now):
    Sikorsky X-2 model is -y forward, x-left and z-up
    ardupilot plugin requires x-forward, y-left, z-up
    for things to work properly, so it's important to set pose below
  -->
  <model name='sikorsky-X2'>
    <pose>0 0 0.2 1.57 0 0</pose>
    <link name='base_link'>
      <velocity_decay>
        <linear>0.0</linear>
        <angular>0.0</angular>
      </velocity_decay>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1.2</mass>
        <inertia>
          <ixx>0.008</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.015</iyy>
          <iyz>0</iyz>
          <izz>0.017</izz>
        </inertia>
      </inertial>
      <collision name='base_link_collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
					<mesh>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Fuselage.dae</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode>
              <max_vel>100.0</max_vel>
              <min_depth>0.001</min_depth>
            </ode>
          </contact>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name='base_link_visual'>
				<pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Fuselage.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Grey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
    </link>
    <link name="virtual_landing">
      <pose>0 0 -0.12 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.0043137104</ixx>
          <ixy>0</ixy>
          <iyy>0.00387382402</iyy>
          <ixz>0</ixz>
          <iyz>0</iyz>
          <izz>0.00809845106</izz>
        </inertia>
      </inertial>
      <collision name="virtual_landing_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.3 .05</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <ode>
              <kp>100000000</kp>
              <kd>1</kd>
              <min_depth>0.001</min_depth>
              <max_vel>1.0</max_vel>
            </ode>
          </contact>
          <friction>
            <ode>
              <!--
              <slip2>20</slip2>
              -->
              <mu>2</mu>
              <mu2>1</mu2>
              <fdir1>1 0 0</fdir1>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>
    <joint name="skid_pad_joint" type="fixed">
      <parent>base_link</parent>
      <child>virtual_landing</child>
      <axis>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
        </limit>
        <xyz>1 0 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    <link name='imu_link'>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.15</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.15</iyy>
          <iyz>0</iyz>
          <izz>0.15</izz>
        </inertia>
      </inertial>
      <sensor name="imu_sensor" type="imu">
        <pose>0 0 0 3.141593 0 -1.57079</pose>
        <always_on>1</always_on>
        <update_rate>1000.0</update_rate>
      </sensor>
    </link>
    <joint name='imu_joint' type='revolute'>
      <child>imu_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>0</lower>
          <upper>0</upper>
          <effort>0</effort>
          <velocity>0</velocity>
        </limit>
        <dynamics>
          <damping>1.0</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <implicit_spring_damper>1</implicit_spring_damper>
        </ode>
      </physics>
    </joint>

        <link name ='swashplate'>
          <pose>0 0.000 0.110 0 0 0</pose>
          <inertial>
            <mass>0.05</mass>
            <inertia>
              <ixx>0.01</ixx>>
              <iyy>0.01</iyy>
              <izz>0.01</izz>
            </inertia>
          </inertial>
        </link>

        <link name ='swashplate2'>
          <pose>0 0.01 0.110 0 0 0</pose>
          <inertial>
            <mass>0.05</mass>
            <inertia>
              <ixx>0.01</ixx>>
              <iyy>0.01</iyy>
              <izz>0.01</izz>
            </inertia>
          </inertial>
          <visual name ='swashplate_visual'>
            <pose>0 0 0.0 0 0 0</pose>
            <geometry>
              <mesh>
                <scale>0.8 0.8 0.8</scale>
                <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Rotor_Hub.dae</uri>
              </mesh>
            </geometry>
            <material>
              <script>
                <name>Gazebo/Turquoise</name>
                <uri>__default__</uri>
              </script>
            </material>
          </visual>
        </link>

    <link name='blade_up'>
      <pose>0 0 0.3 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>0.040</mass>
        <inertia>
          <ixx>9.75e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000166704</iyy>
          <iyz>0</iyz>
          <izz>0.000167604</izz>
        </inertia>
      </inertial>
      <kinematic>1</kinematic>

      <!---
      <collision name='blade_up_collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Up.dae</uri>
          </mesh>
        </geometry>
      </collision>
      !--->
      <visual name='blade_up_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>10 10 10</scale>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Up2.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
      <visual name = 'CP_Visual_up'>
        <pose>0.21 -0.014 0.104 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>0.01</radius>
            </sphere>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Red</name>
              <uri>__default__</uri>
            </script>
          </material>
        </visual>
        <visual name = 'CP_Visual_up2'>
          <pose>-0.21 -0.010 0.104 0 0 0</pose>
            <geometry>
              <sphere>
                <radius>0.01</radius>
              </sphere>
            </geometry>
            <material>
              <script>
                <name>Gazebo/Red</name>
                <uri>__default__</uri>
              </script>
            </material>
          </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>

    <link name='blade_down'>
      <pose>0.005 0.01 -0.055 0 0 0</pose>
      <inertial>
        <mass>0.04</mass>
        <inertia>
          <ixx>9.75e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000166704</iyy>
          <iyz>0</iyz>
          <izz>0.000167604</izz>
        </inertia>
      </inertial>
      <kinematic>1</kinematic>
      <!--
      <collision name='blade_down_collision'>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <mesh>
              <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Down.dae</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision>
    -->
      <visual name='blade_down_visual'>
        <pose>0 0 0 1.57 0 0</pose>
        <geometry>
          <mesh>
							<uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Down.dae</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <name>Gazebo/DarkGrey</name>
            <uri>__default__</uri>
          </script>
        </material>
      </visual>
      <visual name = 'CP_Visual_Down'>
        <pose>0.165 0.14 0.143 0 0 0</pose>
          <geometry>
            <sphere>
              <radius>0.01</radius>
            </sphere>
          </geometry>
          <material>
            <script>
              <name>Gazebo/White</name>
              <uri>__default__</uri>
            </script>
          </material>
        </visual>
        <visual name = 'CP_Visual_Down2'>
          <pose>-0.15 -0.16 0.143 0 0 0</pose>
            <geometry>
              <sphere>
                <radius>0.01</radius>
              </sphere>
            </geometry>
            <material>
              <script>
                <name>Gazebo/White</name>
                <uri>__default__</uri>
              </script>
            </material>
          </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>

    <link name='blade_rear'>
      <pose>0 0 0 1.57 -0 0</pose>
      <inertial>
        <pose>0 0 0 0 -0 0</pose>
        <mass>0.004</mass>
        <inertia>
          <ixx>9.75e-06</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000166704</iyy>
          <iyz>0</iyz>
          <izz>0.000167604</izz>
        </inertia>
      </inertial>
      <collision name='blade_rear_collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Rear.dae</uri>
          </mesh>
        </geometry>
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision>
      <visual name='blade_rear_visual'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://sikorsky-X2/meshes/Sikorsky-X2_Blade_Rear.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </link>


    <!- Control Surfaces Joints -->
    <joint name='blade_up_joint' type='revolute'>
      <child>blade_up</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.004</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <joint name='blade_down_joint' type='revolute'>
      <child>blade_down</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.001</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <joint name='blade_rear_joint' type='revolute'>
      <pose>0 0 0 0 0 0</pose>
      <child>blade_rear</child>
      <parent>base_link</parent>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
        </limit>
        <dynamics>
          <damping>0.001</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <joint name='swashplate_joint' type='revolute'>
      <parent>base_link</parent>
      <child>swashplate</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 -1 0 </xyz>
        <limit>
          <lower>-0.2</lower>
          <upper>0.2</upper>
        </limit>
          <dynamics>
              <damping>0.7</damping>
          </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
        </axis>
    </joint>

    <joint name='swashplate2_joint' type='revolute'>
      <parent>swashplate</parent>
      <child>swashplate2</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>-1 0 0 </xyz>
        <limit>
          <lower>-0.2</lower>
          <upper>0.2</upper>
        </limit>
        <dynamics>
        <damping>0.7</damping>
        </dynamics>
        <use_parent_model_frame>1</use_parent_model_frame>
      </axis>
    </joint>

    <!-- plugins -->
    <!-- Uses same parameter from 3DR Iris-->
    <plugin name="Blade_Up_Plugin_01" filename="libLiftDragPlugin.so">
      <a0>0.1</a0>
      <alpha_stall>0.35</alpha_stall>
      <cla>5.73</cla>
      <cda>0.08</cda>
      <cma>0.00</cma>
      <cla_stall>-0.25</cla_stall>
      <cda_stall>0.0</cda_stall>
      <cma_stall>0.0</cma_stall>
      <area>0.002</area>
      <air_density>1.225</air_density>
      <cp>0.21 -0.014 0.104</cp>
      <forward>0 1 0</forward>
      <upward>0 0 1</upward>
      <link_name>blade_up</link_name>
    </plugin>

    <plugin name="Blade_Up_Plugin_02" filename="libLiftDragPlugin.so">
      <a0>0.1</a0>
      <alpha_stall>0.35</alpha_stall>
      <cla>5.73</cla>
      <cda>0.08</cda>
      <cma>0.00</cma>
      <cla_stall>-0.25</cla_stall>
      <cda_stall>0.0</cda_stall>
      <cma_stall>0.0</cma_stall>
      <area>0.002</area>
      <air_density>1.225</air_density>
      <cp>-0.21 -0.010 0.104</cp>
      <forward>0 -1 0</forward>
      <upward>0 0 1</upward>
      <link_name>blade_up</link_name>
    </plugin>

    <plugin name="Blade_Down_Plugin_01" filename="libLiftDragPlugin.so">
      <a0>0.1</a0>
      <alpha_stall>0.35</alpha_stall>
      <cla>5.73</cla>
      <cda>0.08</cda>
      <cma>0.00</cma>
      <cla_stall>-0.25</cla_stall>
      <cda_stall>0.0</cda_stall>
      <cma_stall>0.0</cma_stall>
      <area>0.002</area>
      <air_density>1.225</air_density>
      <cp>0.165 0.165 0.143</cp>
      <forward>0.707 0.707 0</forward>
      <upward>0 0 1</upward>
      <link_name>blade_down</link_name>
    </plugin>

    <plugin name="Blade_Down_Plugin_02" filename="libLiftDragPlugin.so">
      <a0>0.1</a0>
      <alpha_stall>0.35</alpha_stall>
      <cla>5.73</cla>
